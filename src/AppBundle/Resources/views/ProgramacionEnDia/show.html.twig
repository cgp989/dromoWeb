{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ProgramacionEnDia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Cantidaddisponible</th>
                <td>{{ entity.cantidadDisponible }}</td>
            </tr>
            <tr>
