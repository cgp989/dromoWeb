            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('programacionendia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('programacionendia_edit', { 'id': entity.id }) }}">
